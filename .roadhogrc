{
  "entry": "src/index.js",
  "publicPath": "./",
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
        ["import", { "libraryName": "antd", "libraryDirectory": "lib", "style": "css" }]
      ],
      "proxy": {
        "/api": {
          "target": "http://jsonplaceholder.typicode.com/",
          "changeOrigin": true,
          "pathRewrite": { "^/api" : "" }
        }
      }
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": true  }]
      ]
    }
  },
  "dllPlugin": {
   "exclude": [
     "babel-runtime"
   ],
   "include": [
     "dva/router",
     "dva/saga",
     "dva/fetch"
   ]
  }
}
